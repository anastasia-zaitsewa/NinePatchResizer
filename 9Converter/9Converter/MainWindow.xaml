<Window x:Class="_9Converter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel Name="stackPanel1"
            Height="30" 
            HorizontalAlignment="Left" 
            Margin="0,0,0,0"  
            VerticalAlignment="Top" 
            Orientation="Horizontal">
            <Button Name="button3"
                Content="All Images" 
                Height="23" 
                HorizontalAlignment="Left"  
                Width="75" 
                Click="button3_Click" />
            <Button Name="button1"
                Content="Get XML" 
                Height="23" 
                HorizontalAlignment="Left" 
                Width="75" />
        </StackPanel>
        <ListBox Margin="0,30,0,0" Name="lstbDragAndDrop" AllowDrop="True" DragEnter="lstbDragAndDrop_DragEnter" Drop="lstbDragAndDrop_Drop" ToolTip="DragDrop Your Images or 9Patches">
            
        </ListBox>
    </Grid>
</Window>
