<Window x:Class="_9Converter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="9 Patch Resizer" Height="350" Width="525">
    <Grid>
        <ListBox Name="lstbDragAndDrop"
            AllowDrop="True" 
            DragEnter="lstbDragAndDrop_DragEnter" 
            Drop="lstbDragAndDrop_Drop"/>
        <TextBlock Name="txtHint"
            Text="DragDrop Your Images or 9Patches" 
            VerticalAlignment="Center" 
            HorizontalAlignment="Center" 
            AllowDrop="True" 
            DragEnter="lstbDragAndDrop_DragEnter" 
            Drop="lstbDragAndDrop_Drop"
            Height="20"
            Width="Auto" 
            />
        <ProgressBar Name="progressBar1"
            Height="10"  
            Width="100"
            
            />
    </Grid>
</Window>
